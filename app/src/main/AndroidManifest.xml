<?xml version="1.0" enconding="utf-8"?>

<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.touchablet.Market_bangalore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Market_Bangalore"
        android:supportsRt1="true"
        android:theme= "@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name="com.example.android.singlescreenapp.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.action.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
